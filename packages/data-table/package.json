{
  "name": "@remix-run/data-table",
  "version": "0.1.0",
  "description": "A typed, relational query toolkit for Remix",
  "author": "Michael Jackson <mjijackson@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remix-run/remix.git",
    "directory": "packages/data-table"
  },
  "homepage": "https://github.com/remix-run/remix/tree/main/packages/data-table#readme",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "src",
    "!src/**/*.test.ts"
  ],
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./migrations": "./src/migrations.ts",
    "./migrations/node": "./src/migrations/node.ts",
    "./operators": "./src/operators.ts",
    "./sql": "./src/sql.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./migrations": {
        "types": "./dist/migrations.d.ts",
        "default": "./dist/migrations.js"
      },
      "./migrations/node": {
        "types": "./dist/migrations/node.d.ts",
        "default": "./dist/migrations/node.js"
      },
      "./operators": {
        "types": "./dist/operators.d.ts",
        "default": "./dist/operators.js"
      },
      "./sql": {
        "types": "./dist/sql.d.ts",
        "default": "./dist/sql.js"
      },
      "./package.json": "./package.json"
    }
  },
  "devDependencies": {
    "@remix-run/data-schema": "workspace:*",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "better-sqlite3": "^12.4.1"
  },
  "dependencies": {
    "@remix-run/data-schema": "workspace:^"
  },
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "clean": "git clean -fdX",
    "prepublishOnly": "pnpm run build",
    "test": "node --disable-warning=ExperimentalWarning --test",
    "test:coverage": "pnpm run test:coverage:core && pnpm run test:coverage:migrations",
    "test:coverage:core": "node --disable-warning=ExperimentalWarning --experimental-test-coverage --test-coverage-include='src/lib/operators.ts' --test-coverage-include='src/lib/references.ts' --test-coverage-include='src/lib/inflection.ts' --test-coverage-include='src/lib/sql.ts' --test-coverage-include='src/lib/errors.ts' --test-coverage-lines=90 --test-coverage-branches=90 --test-coverage-functions=90 --test ./src/lib/operators.test.ts ./src/lib/references.test.ts ./src/lib/inflection.test.ts ./src/lib/sql.test.ts ./src/lib/errors.test.ts",
    "test:coverage:migrations": "node --disable-warning=ExperimentalWarning --experimental-test-coverage --test-coverage-include='src/lib/migrations.ts' --test-coverage-lines=90 --test-coverage-branches=90 --test-coverage-functions=90 --test ./src/lib/migrations.test.ts",
    "typecheck": "tsgo --noEmit"
  },
  "keywords": [
    "remix",
    "orm",
    "sql",
    "database",
    "query-builder",
    "relational"
  ]
}
